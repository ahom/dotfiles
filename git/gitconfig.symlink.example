# Sample gitconfig
#

[user]
    name = AUTHORNAME
    email = AUTHOREMAIL
[credential]
    helper = GIT_CREDENTIAL_HELPER
[alias]
    co = checkout
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[core]
    editor = vim
[help]
    autocorrect = 1
[push]
    default = simple
